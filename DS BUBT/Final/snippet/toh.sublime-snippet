void TOH (int disk, char start, char end, char mid) {
  if (disk == 0) return;
  TOH(disk - 1, start, mid, end);
  cout << disk << " : "<< start << " => " << end << nl;
  TOH(disk - 1, mid, end, start);
}
]]></content>
